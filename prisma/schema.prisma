
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
enum Role {
  USER
  ADMIN
  MODERATOR
}

model AllUsers{
id String  @id @default(uuid())
createdAT  DateTime @default(now())
userName String
age Int?
imgUrl String?
publicId  String?
role Role @default(USER)
}